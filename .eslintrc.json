{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["main.js"],
  "rules": {
    "no-param-reassign": 0,
    "import/extensions": 0,
    "no-console": 0
  }
}
